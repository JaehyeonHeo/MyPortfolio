
@{
    ViewData["Title"] = "Portfolio";
    Layout = "~/Views/Shared/_ProfileLayout.cshtml";
}


<div class="card-header">
    <div class="col-md-auto ml-auto mr-auto text-center">
        <h2 class="title">Portfolio</h2>
        <h5 class="description">작업했던 포트폴리오 내용입니다.</h5>
    </div>
</div>
<div class="card-body">
    <div class="col-md-auto ml-auto mr-auto text-center">
        <div class="profile-tabs">
            <ul class="nav nav-pills nav-pills-primary justify-content-center" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" href="#DesktopApp" role="tab" data-toggle="tab">
                         DesktopApp
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#WebApp" role="tab" data-toggle="tab">
                         WebApp
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>
<div class="tab-content tab-space">
    <div class="tab-pane active text-center gallery" id="DesktopApp">
        <div class="row">
            <div class="col-md-auto ml-auto">
                <img src="~/assets/img/examples/portfolio/AnalogClockApp.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/bookRentalShop2.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/IoTSensorMonApp.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/ListboxApp.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/ListViewApp.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/LoginApp.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/MovieFinder.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/MovieFinder2.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/PhotoSensorMonitorApp.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/SimpleGraphicEditor.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/SMS.png" class="rounded">
                <img src="~/assets/img/examples/portfolio/StudyHistoryApp.png" class="rounded">
            </div>
        </div>
    </div>
    <div class="tab-pane text-center gallery" id="WebApp">
        <div class="row">
            <div class="col-md-auto ml-auto">
                <img src="~/assets/img/examples/portfolioWeb/interest웹페이지.png" class="rounded">
                <img src="~/assets/img/examples/portfolioWeb/interest웹페이지2.png" class="rounded">
                <img src="~/assets/img/examples/portfolioWeb/게시판글쓰기.png" class="rounded">
                <img src="~/assets/img/examples/portfolioWeb/게시판메인.png" class="rounded">
